#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    int numDecodings(const string &s) {
        int n = s.size();
        if (n == 0) return 0;

        int prev2 = 1;
        int prev1 = 0;
        if (s[0] == '0') return 0;
        prev1 = 1;

        for (int i = 2; i <= n; ++i) {
            int cur = 0;
            if (s[i-1] != '0') cur += prev1;
            int two = (s[i-2] - '0') * 10 + (s[i-1] - '0');
            if (two >= 10 && two <= 26) cur += prev2;
            prev2 = prev1;
            prev1 = cur;
        }

        return prev1;
    }
};
